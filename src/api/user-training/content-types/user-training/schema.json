{
  "kind": "collectionType",
  "collectionName": "user_trainings",
  "info": {
    "singularName": "user-training",
    "pluralName": "user-trainings",
    "displayName": "UserTraining"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "dueDate": {
      "type": "date"
    },
    "student": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "user_trainings"
    },
    "training": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::training.training",
      "inversedBy": "userTrainings"
    },
    "completed": {
      "type": "boolean",
      "default": false
    },
    "completeDate": {
      "type": "date"
    },
    "certification": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "videos",
        "audios",
        "files"
      ]
    }
  }
}
